Cell[BoxData[{
 RowBox[{
  RowBox[{"eq2", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"s", "*", 
       RowBox[{"p1", "[", "x", "]"}]}], "\[Equal]", 
      RowBox[{"dd", "*", 
       RowBox[{
        RowBox[{"p1", "''"}], "[", "x", "]"}]}]}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"s", "*", 
        RowBox[{"p2", "[", "x", "]"}]}], "-", 
       RowBox[{"DiracDelta", "[", 
        RowBox[{"x", "-", "z"}], "]"}]}], "\[Equal]", 
      RowBox[{"dd", "*", 
       RowBox[{
        RowBox[{"p2", "''"}], "[", "x", "]"}]}]}], ",", 
     RowBox[{
      RowBox[{"s", "*", 
       RowBox[{"p3", "[", "x", "]"}]}], "\[Equal]", 
      RowBox[{"dd", "*", 
       RowBox[{
        RowBox[{"p3", "''"}], "[", "x", "]"}]}]}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"bc2", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"p1", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
     RowBox[{
      RowBox[{"p3", "[", "L", "]"}], "\[Equal]", "0"}], ",", 
     RowBox[{
      RowBox[{"p1", "[", "a", "]"}], "\[Equal]", 
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
         RowBox[{"-", 
          RowBox[{"(", 
           RowBox[{"\[Mu]", " ", "U"}], ")"}]}], "/", "dd"}]], " ", 
       RowBox[{"p2", "[", "a", "]"}]}]}], ",", 
     RowBox[{
      RowBox[{"p3", "[", "b", "]"}], "\[Equal]", 
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
         RowBox[{"-", 
          RowBox[{"(", 
           RowBox[{"\[Mu]", " ", "U"}], ")"}]}], "/", "dd"}]], " ", 
       RowBox[{"p2", "[", "b", "]"}]}]}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"p1", "'"}], "[", "a", "]"}], "==", 
      RowBox[{
       RowBox[{"p2", "'"}], "[", "a", "]"}]}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"p2", "'"}], "[", "b", "]"}], "\[Equal]", 
